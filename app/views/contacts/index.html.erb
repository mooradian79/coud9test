<h1>Listing all Contacts</h1>

<p>
 <%= link_to 'Home', root_path %> | <%= link_to "Create new contact", new_contact_path %>
</p>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
  </tr>
 
    <% @contacts.each do |contact| %>
     <tr>
       <td><%= contact.first_name %></td>
       <td><%= contact.last_name %></td>
       <td><%= contact.address %></td>
       <td><%= contact.city %></td>
       <td><%= contact.state %></td>
       <td><%= contact.zip %> </td>
       <td><%= link_to "Edit", edit_contact_path(contact) %></td>
       <td><%= link_to "Show", contact_path(contact) %> </td>
       <td><%= link_to "Delete", contact_path(contact), method: :delete, data: {confirm: "Are you sure?"} %></td>
       
       
    </tr>
    
    <% end %>
  
  
</table>